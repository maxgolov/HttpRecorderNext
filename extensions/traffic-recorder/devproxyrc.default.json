{
  "$schema": "https://raw.githubusercontent.com/microsoft/dev-proxy/main/schemas/v1.3.0/rc.schema.json",
  "plugins": [
    {
      "name": "HttpRecorderPlugin",
      "enabled": true,
      "pluginPath": "./bin/plugins/HttpRecorder.DevProxy.dll",
      "configSection": "httpRecorder"
    }
  ],
  "urlsToWatch": [
    "https://*",
    "http://*"
  ],
  "port": 8000,
  "asSystemProxy": false,
  "record": false,
  "httpRecorder": {
    "outputDirectory": ".",
    "mode": "Record",
    "includeBodies": true,
    "anonymizeSensitiveData": true,
    "sensitiveHeaders": [
      "Authorization",
      "Cookie",
      "Set-Cookie",
      "X-API-Key",
      "X-Auth-Token"
    ]
  },
  "logLevel": "information",
  "newVersionNotification": "stable",
  "showMarketplaceMessages": false
}
