{
  "$schema": "https://raw.githubusercontent.com/microsoft/dev-proxy/main/schemas/v1.3.0/rc.schema.json",
  "plugins": [
    {
      "name": "HttpRecorderPlugin",
      "enabled": true,
      "pluginPath": "./bin/plugins/HttpRecorder.DevProxy.dll",
      "configSection": "httpRecorder"
    }
  ],
  "urlsToWatch": [
    "https://*",
    "http://*"
  ],
  "port": 8080,
  "asSystemProxy": false,
  "record": false,
  "httpRecorder": {
    "outputDirectory": ".",
    "mode": "Record",
    "includeBodies": true,
    "anonymizeSensitiveData": true,
    "sensitiveHeaders": [
      "Authorization",
      "Proxy-Authorization",
      "WWW-Authenticate",
      "Proxy-Authenticate",
      "Cookie",
      "Set-Cookie",
      "X-API-Key",
      "X-APIKey",
      "x-api-key",
      "apikey",
      "api-key",
      "api_key",
      "X-Auth-Token",
      "X-Auth",
      "X-Access-Token",
      "X-Refresh-Token",
      "X-Session-Token",
      "X-Session-ID",
      "X-Session-Key",
      "Authentication",
      "X-CSRF-Token",
      "X-XSRF-Token",
      "X-CSRF-Protection",
      "OpenAI-Organization",
      "OpenAI-Project",
      "anthropic-version",
      "X-Amz-Security-Token",
      "X-Azure-Auth",
      "x-goog-api-key",
      "X-OAuth-Token",
      "Authorization-Bearer",
      "X-Forwarded-Authorization",
      "X-Original-Authorization",
      "Authorization-Info"
    ]
  },
  "logLevel": "information",
  "newVersionNotification": "stable",
  "showMarketplaceMessages": false
}
